<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.pj.ebk.dao.EbookTopDao">
	<insert id="saveObject">
		INSERT INTO top
		(id,title,synopsis,author,img_url,pageviews,praise_points) 
		VALUES(null,#{title},#{synopsis},#{author}, 
		#{imgUrl},#{pageviews},#{praisePoints})
	</insert>
</mapper>
